{
  "name": "natcycle_server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon src/server.js",
    "setup:database": "node src/scripts/setupDatabase.js",
    "prestart": "npm run setup:database",
    "build": "npm run setup:database",
    "seed:items": "node src/scripts/seedItems.js",
    "seed:dropoffs": "node src/scripts/seedDropOffLocations.js",
    "seed:campaigns": "node src/scripts/seedCampaigns.js",
    "seed:simple-dropoffs": "node src/scripts/seedSimpleDropOffLocations.js",
    "seed:simple-dropoffs-test": "node src/scripts/runSimpleDropOffSeed.js",
    "seed:materials": "node src/scripts/seedMaterials.js",
    "seed:all": "npm run seed:dropoffs && npm run seed:simple-dropoffs && npm run seed:campaigns && npm run seed:items && npm run seed:materials",
    "seed:force-dropoffs": "node src/scripts/seedDropOffLocations.js 20 force",
    "seed:force-items": "node src/scripts/seedItems.js 30 force",
    "clear:items": "node src/scripts/clearItems.js",
    "clear:dropoffs": "node src/scripts/clearDropOffs.js",
    "clear:campaigns": "node src/scripts/clearCampaigns.js"
  },
  "author": "Jarvis Dev",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.33.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.4.0",
    "google-auth-library": "^9.4.1",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.7.3",
    "mongoose-paginate-v2": "^1.8.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.0.5",
    "nodemailer": "^6.9.3",
    "redis": "^4.7.0",
    "request-ip": "^3.3.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "http-status": "^2.1.0",
    "nodemon": "^3.1.10",
    "winston": "^3.17.0"
  }
